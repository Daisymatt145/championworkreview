<h2>Edit champion work</h2>
<%= form_for @championwork do |f| %>
<p><% if @championwork.errors.any? %></p>
  <h2>Erros</h2>
  <ul>
   <% @championwork.errors.full_messages.each do |message|  %>
       <li><%= message %></li>
   <% end %>
  </ul>
<% end %>
<p><%= f.label :workcategory %><br/>
<%= f.text_field :workcategory %>
</p>
<p>
<%= f.label :Description %><br/>
<%= f.text_area :description %>
</p>
</p>
<%= f.submit "Update a Champion work" %>
</p>
<%end%>